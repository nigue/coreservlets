<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <navigation-rule>
        <from-view-id>jsf/navigation/autocomplete-1.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-language</from-outcome>
            <to-view-id>jsf/pages/show-language.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/autocomplete-2.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-language</from-outcome>
            <to-view-id>jsf/pages/show-language.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/autocomplete-3.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-languages</from-outcome>
            <to-view-id>jsf/pages/show-languages.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/input-mask.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-input-mask-data</from-outcome>
            <to-view-id>jsf/pages/show-input-mask-data.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
