<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>jsf/navigation/colorpicker.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-colors</from-outcome>
            <to-view-id>jsf/pages/show-colors.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/inplace.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-colors</from-outcome>
            <to-view-id>jsf/pages/show-colors.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/captcha.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-registration-1</from-outcome>
            <to-view-id>jsf/pages/show-registration-1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/password.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-registration-2</from-outcome>
            <to-view-id>jsf/pages/show-registration-2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/editor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show-message</from-outcome>
            <to-view-id>jsf/pages/show-message.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
