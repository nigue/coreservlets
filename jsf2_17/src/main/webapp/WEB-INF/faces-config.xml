<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>msgs</var>
        </resource-bundle>
    </application>
    
    <navigation-rule>
        <from-view-id>jsf/navigation/resort-registration-1</from-view-id>
        <navigation-case>
            <from-outcome>show-dates-1</from-outcome>
            <to-view-id>jsf/pages/show-dates-1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>jsf/navigation/resort-registration-2</from-view-id>
        <navigation-case>
            <from-outcome>show-dates-2</from-outcome>
            <to-view-id>jsf/pages/show-dates-2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
        <navigation-rule>
        <from-view-id>jsf/navigation/calculator</from-view-id>
        <navigation-case>
            <from-outcome>show-sum</from-outcome>
            <to-view-id>jsf/pages/show-sum.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
        <navigation-rule>
        <from-view-id>jsf/navigation/bug-report-1</from-view-id>
        <navigation-case>
            <from-outcome>report-confirmation</from-outcome>
            <to-view-id>jsf/pages/report-confirmation.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
